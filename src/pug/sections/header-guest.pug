- if (!login) var login = "login"
- if (!register) var register = "register"
//- - if (!headerMenu) var headerMenu = [{itemName:"Home"}, "About us", "Services", "Careers", " News", "Documentation"];
//- - if (!headerMenu) var headerMenu = {"Home":"0", "About us":"1", "Services":["first", "second", "third"], "Careers":"3", "News":"4", "Documentation":"5"};
- var Services = ["first", "second", "third"]
//- - if (!itemNumberList) var itemNumberList = "2";
- if (!itemNumberActive) var itemNumberActive = "0";

-
	if (!headerMenu) var headerMenu = [
		{title: "Home", url: "#", submenu: ""},
		{title: "About us", url: "#", submenu: ""},
		{title: "Services", url: "#", submenu: Services},
		{title: "Careers", url: "#", submenu: ""},
		{title: "News", url: "#", submenu: ""},
		{title: "Documentation", url: "#", submenu: ""}
	]

.header
	.header__logo.logo
		.logo__icon(style="background-image: url(./assets/img/icons/logo.png);")
		.logo__text(style="background-image: url(./assets/img/icons/toxin.png);")
	.header__nav
		//- - var 
		ul.navigation
			each val in headerMenu
				if (val.submenu != "")
					li.navigation__item.navigation__item--chevron #{val.title} 
						i.zmdi.zmdi-chevron-down.navigation-chevron
						ul.inner-list
							each elem in val.submenu
								li.inner-list__item #{elem}

				else
					li.navigation__item #{val.title}
		.header-login
			.header-login__guest
				.header-login__guest-login
					+button-secondary(login, "#", "small")
				.header-login__guest-register
					+button-primary(register, "#", "small")



